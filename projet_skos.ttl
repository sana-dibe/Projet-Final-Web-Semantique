:TarifEntree rdf:type skos:Concept ;
    skos:inScheme :HistoireEtLieuxHistoriques ;
    skos:prefLabel "Tarif d'entrée" ;
    skos:altLabel "Prix, coût, tarif" ;
    skos:definition "Le coût pour entrer dans un lieu historique ou culturel" ;
    skos:related :Remise, :BilletFamille, :GratuitPourLesEnfants ;
    skos:narrower :TarifNormal, :TarifReduit, :TarifGratuit .

:HoraireOuverture rdf:type skos:Concept ;
    skos:inScheme :HistoireEtLieuxHistoriques ;
    skos:prefLabel "Horaire d'ouverture" ;
    skos:definition "Les horaires d'ouverture d'un site historique ou culturel" ;
    skos:related :SiteHistorique, :Fermeture, :JoursOuverture .

